{
    "name": "IDE",
    "id": "ide",
    "version": "0.0.1",
    "description": "Install an IDE in the devcontainer",
    "options": {
        "ide": {
            "type": "string",
            "enum": [
                "rstudio-server",
                "jupyterlab",
                "code-server"
            ],
            "default": "code-server",
            "description": "Select your favorite IDE"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/common-utils"
    ]
}
